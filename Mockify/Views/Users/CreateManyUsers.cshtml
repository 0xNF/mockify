@model Mockify.ViewModels.CreateManyUsersViewModel

<h4>Create Many Users</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateUsers" asp-controller="Users">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CreateThisManyUsers" class="control-label"></label>
                <input asp-for="CreateThisManyUsers" class="form-control" />
                <span asp-validation-for="CreateThisManyUsers" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>

        Or, import from a CSV.
        <div>
            <input type="file"/>
        </div>
        <p>
            The CSV format is:
            id, display_name, password, email, birthdate, country, product_type
        </p>
    </div>
</div>

